{
    "name": "mikespub/micheh-psr7-cache",
    "description": "Cache and conditional request helpers for PSR-7 HTTP Messages",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "cache",
        "http",
        "psr",
        "psr-7",
        "http-message",
        "request",
        "response",
        "conditional",
        "if-match",
        "if-none-match",
        "if-modified-since",
        "if-unmodified-since"
    ],
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.1 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^11.5 || ^12.1",
        "phpstan/phpstan": "^1.12 || ^2.1"
    },
    "autoload": {
        "psr-4": {
            "Micheh\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichehTest\\Cache\\": "test/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "coverage": "vendor/bin/phpunit --coverage-text",
        "coverage-html": "vendor/bin/phpunit --coverage-html=coverage"
    }
}
